{
  "name": "imamsudarajat04/laravel-change-logs",
  "description": "A comprehensive change logs tracking package for Laravel 11+",
  "keywords": ["laravel", "change-logs", "audit", "tracking"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Imam Sudarajat",
      "email": "imamsudarajat708@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0",
    "illuminate/database": "^11.0",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Imamsudarajat04\\ChangeLogs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Imamsudarajat04\\ChangeLogs\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Imamsudarajat04\\ChangeLogs\\ChangeLogsServiceProvider"
      ],
      "aliases": {
        "ChangeLog": "Imamsudarajat04\\ChangeLogs\\Facades\\ChangeLog"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}